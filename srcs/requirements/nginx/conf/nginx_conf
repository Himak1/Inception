user nginx;

worker_processes  1;
error_log  /var/log/nginx/error.log warn;
pid        /var/run/nginx.pid;

events {
    worker_connections  1024;
}

http {
    server {
        listen 80;

        location / {
            gzip off;
            root /conf/;
            index index.html;
        }
    }
    sendfile on;
    keepalive_timeout 65;
}
